<form action="/cats/<%=@cat['id']%>/edit" method="post">
    <label for="">Name</label>
    <input type="text" name="name" value="<%=@cat['name'] %>"

    <label for="">PHOTOURL:</label>
    <input type="text" name="photo" value="<%=@cat['photo']%>">

    <label>Breed</label>
    <input type="text" name="breed" value="<%=@cat['breed']%>">
    <button>New Cat</button>
</form>